{
  "name": "nanofuzz",
  "version": "0.1.0",
  "displayName": "NaNofuzz",
  "description": "Numeric Fuzzer for Typescript",
  "repository": "https://github.com/penrose/nanofuzz.git",
  "author": "Penrose Team (https://penrose.cs.cmu.edu/)",
  "license": "MIT",
  "main": "build/extension/extension.js",
  "module": "build/dist/index.esm.js",
  "unpkg": "build/dist/penrose.min.js",
  "types": "build/dist/index.d.ts",
  "type": "commonjs",
  "files": [
    "/build"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "modulePaths": [
      "node_modules",
      "<rootDir>/src/"
    ],
    "testPathIgnorePatterns": [
      "build/",
      "src/test/suite/"
    ]
  },
  "engines": {
    "vscode": "^1.69.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:nanofuzz.Fuzz"
  ],
  "contributes": {
    "commands": [
      {
        "command": "nanofuzz.Fuzz",
        "title": "Fuzz"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "nanofuzz.Fuzz",
          "group": "z_commands",
          "when": "editorLangId == typescript && editorTextFocus"
        }
      ]
    },
    "keybindings": [
      {
        "command": "nanofuzz.Fuzz",
        "key": "ctrl+alt+f",
        "mac": "ctrl+alt+f",
        "when": "editorLangId == typescript && editorTextFocus"
      }
    ]
  },
  "scripts": {
    "build": "node build.js",
    "test": "jest --watchAll=false",
    "start": "yarn build;node ./build/dist/index.esm.js",
    "docs": "typedoc --plugin none --out docs",
    "coverage": "yarn test --coverage --watchAll=false",
    "lint": "eslint --ext js,ts,tsx src",
    "build-decls": "tsc --declaration --outDir build/dist && tsc-alias -p tsconfig.json",
    "vscode:prepublish": "yarn run compile",
    "compile": "yarn build",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "ext:test": "node ./build/extension/test/runTest.js"
  },
  "dependencies": {
    "@datastructures-js/queue": "^4.2.1",
    "@vscode/webview-ui-toolkit": "^1.0.0",
    "eslint": "^7.3.2",
    "immutable": "^4.1.0",
    "seedrandom": "^3.0.5",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "16.x",
    "@types/seedrandom": "^2.4.28",
    "@types/vscode": "^1.69.0",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "@vscode/test-electron": "^2.1.5",
    "esbuild": "^0.14.48",
    "escape-goat": "^4.0.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.3",
    "eslint-plugin-react": "^7.28.0",
    "estrella": "^1.4.1",
    "jest": "^27.5.1",
    "jest-junit": "^12.0.0",
    "ts-jest": "^27.1.3",
    "tsc-alias": "^1.6.7",
    "tslib": "^2.1.0",
    "typedoc": "^0.22.11",
    "typedoc-plugin-markdown": "^3.11.13"
  }
}
